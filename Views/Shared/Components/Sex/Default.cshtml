@{
}

@model IEnumerable<string>

<div class="d-grid gap-2">
    <h3>Select Sex</h3>

    <a asp-controller="Home" asp-action="Burials" asp-route-sex="" asp-route-pageNum="1" class="btn btn-outline-secondary">All</a>

    @foreach (string c in Model)
    {
        <a asp-controller="Home" asp-action="Burials" asp-route-sex="@c" asp-route-pageNum="1"
           class="btn @(ViewBag.SelectedSex == c ? "btn-primary": "btn-outline-secondary")">@c</a>
    }
</div>